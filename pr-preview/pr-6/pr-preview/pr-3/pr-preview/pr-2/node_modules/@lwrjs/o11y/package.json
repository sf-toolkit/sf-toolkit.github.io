{
    "name": "@lwrjs/o11y",
    "license": "MIT",
    "publishConfig": {
        "access": "public"
    },
    "version": "0.12.4",
    "homepage": "https://developer.salesforce.com/docs/platform/lwr/overview",
    "repository": {
