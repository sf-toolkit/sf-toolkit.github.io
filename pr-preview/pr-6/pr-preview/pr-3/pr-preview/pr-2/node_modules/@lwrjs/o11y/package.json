    },
    "lwc": {
        "modules": [
            {
                "dir": "build/modules"
            },
            {
                "npm": "o11y"
            }
        ],
        "expose": [
            "lwr/o11yService"
        ]
    },
    "engines": {
        "node": ">=18.0.0"
